{
  "name": "nuxt-build-info",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
  "description": "Track deployments with build-time info: version, git details, env context and timestamps for Nuxt apps",
  "author": "Phojie Rengel <phojrengel@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/phojie",
  "homepage": "https://github.com/phojie/nuxt-build-info",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phojie/nuxt-build-info.git"
  },
  "bugs": "https://github.com/phojie/nuxt-build-info/issues",
  "keywords": [
    "nuxt",
    "build",
    "env",
    "version",
    "git",
    "details",
    "env",
    "context",
    "timestamps"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm run lint && pnpm run test && pnpm run prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.2",
    "simple-git": "^3.16.0",
    "std-env": "^1.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.0",
    "@nuxt/devtools": "^2.3.2",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.2",
    "@nuxt/test-utils": "^3.17.2",
    "@types/node": "latest",
    "changelogen": "^0.6.1",
    "eslint": "^9.26.0",
    "nuxt": "^3.17.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  }
}
